  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "typescript": "^5.7.3",
    "@types/node": "^22.13.4",
    "shx": "^0.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
