{
  "name": "@21st-dev/magic",
  "version": "0.0.9",
  "type": "module",
  "description": "Magic MCP UI builder by 21st.dev",
  "homepage": "https://21st.dev/magic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/21st-dev/magic-mcp.git"
  },
  "bin": {
    "magic": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "UI",
    "frontend",
    "21st.dev",
    "21",
    "magic"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "start": "node dist/index.js",
